{
  "name": "valorant-instalocker",
  "version": "1.0.0",
  "description": "Valorant Agent Instalocker",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-win": "electron-builder --win --x64"
  },
  "keywords": ["valorant", "instalocker", "agent"],
  "author": "YourName",
  "license": "MIT",
  "build": {
    "appId": "com.valorant.instalocker",
    "productName": "Valorant Instalocker",
    "electronDownload": {
      "mirror": "https://github.com/electron/electron/releases/download/"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "icon": "icon.ico",
      "certificateFile": null,
      "certificatePassword": null,
      "signingHashAlgorithms": [],
      "extraResources": [
        {
          "from": "node_modules/electron/dist",
          "to": ".",
          "filter": ["ffmpeg.dll"]
        }
      ]
    },
    "files": [
      "main.js",
      "renderer.js",
      "index.html",
      "package.json"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Valorant Instalocker"
    },
    "portable": {
      "artifactName": "ValorantInstalocker-Portable.exe",
      "unpackDirName": "ValorantInstalocker"
    },
    "directories": {
      "output": "dist"
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "combined-stream": "^1.0.8",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.9.1"
  }
}